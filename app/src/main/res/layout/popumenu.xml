<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="320.0px"
    android:layout_height="420.0px"
    >
    <ImageView
        android:layout_width="300.0px"
        android:layout_height="420.0px"
        android:background="@drawable/popmunt"

        />
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="300px"
        android:layout_height="420px"
        android:orientation="vertical"
     android:layout_alignParentStart="true"
        >
        <TextView
            android:id="@+id/pop_liebiao_tv1"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:text="全部"
            android:gravity="center_vertical"
            android:layout_marginLeft="34px"
            android:paddingTop="20px"

            />
        <TextView
            android:layout_width="300px"
            android:layout_height="2px"
            android:background="#eeeeee"
            />
        <TextView
            android:id="@+id/pop_liebiao_tv2"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:text="分类一"
            android:gravity="center_vertical"
            android:layout_marginLeft="34px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#eeeeee"
            android:layout_below="@id/pop_liebiao_tv"
            />
        <TextView
            android:id="@+id/pop_liebiao_tv3"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:text="分类一"
            android:gravity="center_vertical"
            android:layout_marginLeft="34px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#eeeeee"
            android:layout_below="@id/pop_liebiao_tv"
            />
        <TextView
            android:id="@+id/pop_liebiao_tv4"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:text="分类一"
            android:gravity="center_vertical"
            android:layout_marginLeft="34px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#eeeeee"
            android:layout_below="@id/pop_liebiao_tv"
            />
        <TextView
            android:id="@+id/pop_liebiao_tv5"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:text="分类一"
            android:gravity="center_vertical"
            android:layout_marginLeft="34px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="#eeeeee"
            android:layout_below="@id/pop_liebiao_tv"
            />
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoRelativeLayout>
